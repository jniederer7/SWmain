{
  "name": "stock-watch",
  "version": "1.0.0",
  "description": "MERN stack SPA using WebSockets to display live data related to trading stocks",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mongodb": "^3.0.1",
    "morgan": "^1.9.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "dotenv": "^4.0.0",
    "react-scripts": "^1.0.17"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=development babel universal.js -o universal-compiled.js && NODE_ENV=development babel client/src -d lib --ignore *.test.js",
    "start:dev": "concurrently \"node server\" \"npm run client:dev\"",
    "start": "concurrently \"node server\" \"npm run client\"",
    "server": "node server.js",
    "client:dev": "NODE_ENV=development babel-node start-client.js",
    "client": "node start-client.js",
    "test": "node test-client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jniederer7/Project3"
  },
  "author": "Julian Hasse",
  "license": "ISC",
